msgctxt "Multisite active signup type"
msgid "all"
msgstr "tots"

#: wp-signup.php:389
msgctxt "Multisite active signup type"
msgid "none"
msgstr "cap"

#: wp-signup.php:390
msgctxt "Multisite active signup type"
msgid "blog"
